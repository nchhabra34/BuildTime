<?xml version='1.1' encoding='UTF-8'?>
<project>
  <actions/>
  <description>MongoService STB build</description>
  <keepDependencies>false</keepDependencies>
  <properties>
    <hudson.security.AuthorizationMatrixProperty>
      <inheritanceStrategy class="org.jenkinsci.plugins.matrixauth.inheritance.InheritParentStrategy"/>
      <permission>hudson.model.Item.Build:mongodev</permission>
      <permission>hudson.model.Item.Cancel:mongodev</permission>
      <permission>hudson.model.Item.Read:mongodev</permission>
    </hudson.security.AuthorizationMatrixProperty>
  </properties>
  <scm class="hudson.scm.NullSCM"/>
  <assignedNode>192.168.210.72</assignedNode>
  <canRoam>false</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers>
    <hudson.triggers.TimerTrigger>
      <spec>00      8      *     *   1,2,3,4,5,6</spec>
    </hudson.triggers.TimerTrigger>
  </triggers>
  <concurrentBuild>false</concurrentBuild>
  <customWorkspace>/opt/build/src/razor_mongoservice</customWorkspace>
  <builders>
    <hudson.tasks.Shell>
      <command>source ~/.bash_profile
cd /opt/build/src/razor_mongoservice/mongoservice/build_scripts/stabilization
source build.conf
sh commonmongoservicebuild.sh build</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers>
    <hudson.tasks.Mailer plugin="mailer@1.21">
      <recipients>Release-Team@yodlee.com</recipients>
      <dontNotifyEveryUnstableBuild>false</dontNotifyEveryUnstableBuild>
      <sendToIndividuals>false</sendToIndividuals>
    </hudson.tasks.Mailer>
  </publishers>
  <buildWrappers/>
</project>